<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ServerOverviewController">
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
        <children>
            <Label layoutX="199.0" layoutY="39.0" text="SERVER OVERVIEW">
                <font>
                    <Font size="25.0" />
                </font>
            </Label>
            <Button layoutX="35.0" layoutY="39.0" mnemonicParsing="false" onAction="#onBack" text="BACK" />
            <Label layoutX="45.0" layoutY="111.0" text="Number of users:" />
            <Label fx:id="numberOfUsers" layoutX="188.0" layoutY="111.0" text="-numberOfUsers-" />
            <Label layoutX="44.0" layoutY="157.0" text="Number of offers:" />
            <Label fx:id="numberOfOffers" layoutX="187.0" layoutY="157.0" text="-numberOfOffers-" />
            <Label layoutX="23.0" layoutY="202.0" text="Number of closed deals:" />
            <Label fx:id="numberOfClosedDeals" layoutX="188.0" layoutY="202.0" text="-numberOfClosedDeals-" />
            <Label layoutX="23.0" layoutY="370.0" text="Development team and credits:">
                <font>
                    <Font name="System Bold Italic" size="15.0" />
                </font>
            </Label>
            <Label layoutX="325.0" layoutY="299.0" text="Nicola Santolini" />
            <Label layoutX="325.0" layoutY="323.0" text="Abdirahman Hassan" />
            <Label layoutX="325.0" layoutY="347.0" text="Dragos Baias" />
            <Label layoutX="325.0" layoutY="372.0" text="Nicolae Cernean" />
            <Label layoutX="39.0" layoutY="241.0" text="Number of tenants:" />
            <Label layoutX="34.0" layoutY="272.0" text="Number of landlords:" />
            <Label fx:id="numberOfTenants" layoutX="202.0" layoutY="241.0" text="-numberOfTenants-" />
            <Label fx:id="numberOfLandlords" layoutX="199.0" layoutY="272.0" text="-numberOfLandlords-" />
        </children>
    </AnchorPane>
</VBox>
